block page-js

block page-navbar
    +navbar('team')

mixin member(name, pp, desg, text, ...links)
    .masonry--panel
        img.img-thumbnail(class='img-thumbnail' src='/assets/images/profile/'+pp alt='Photo of '+name)
        p.name #{name}
        p.desg #{desg}
        .links
            each link in links
                if (link.title == 'Researchgate')
                    a(href=link.link target='_blank' title='Researchgate')
                        .fa.fa-researchgate-square
                else if (link.title == 'LinkedIn')
                    a(href=link.link target='_blank' title='LinkedIn')
                        .fa.fa-linkedin-square
        .extra-info-wrapper
            p.text !{text}

block page-wrapper
