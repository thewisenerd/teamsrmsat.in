block page-navbar
    +navbar('team')

mixin minion1(name, pp, desg)
    .xs-twelve.sm-four.md-three.column.minion.minion--1
        img.img-thumbnail(class='img-thumbnail' src='/assets/images/'+pp alt='Photo of '+name)
        p.name #{name}
        p.desg #{desg}

mixin minion2(name, pp, desg)
    .xs-six.sm-three.md-two.column.minion.minion--2
        img.img-thumbnail(class='img-thumbnail' src='/assets/images/'+pp alt='Photo of '+name)
        p.name #{name}
        p.desg #{desg}

