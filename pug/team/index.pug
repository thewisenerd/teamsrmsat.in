        .container
            .row.u-text-center
                ul#slidesMenu.section-heading
                    li(data-menuanchor='core')
                        a.u-heading-slab.active(data-src='core' href='javascript:void(0)') Core Team
                    li(data-menuanchor='sfd')
                        a.u-heading-slab(data-src='sfd' href='javascript:void(0)') Spaceflight Dynamics
                    li(data-menuanchor='astrionics')
                        a.u-heading-slab(data-src='astrionics' href='javascript:void(0)') Astrionics
                    li(data-menuanchor='mechsystems')
